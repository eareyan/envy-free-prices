CREATE TABLE unit_demand (
	n integer NOT NULL,
	m integer NOT NULL,
	p double precision NOT NULL,
	maxWEQRevenue double precision NOT NULL,
	maxWEQTime double precision NOT NULL,
	evpAppRevenue double precision NOT NULL,
	evpAppTime double precision NOT NULL,
	lpRevenue double precision NOT NULL,
	lpTime double precision NOT NULL,
	lpWEViolations double precision NOT NULL,
	lpWERelativeViolations double precision NOT NULL,
	lpEFViolations double precision NOT NULL
	);

CREATE TABLE unit_comparison (
	n integer NOT NULL,
	m integer NOT NULL,
	p double precision NOT NULL,
	maxWEQRevenue double precision NOT NULL,
	maxWEQTime double precision NOT NULL,
	evpAppRevenue double precision NOT NULL,
	evpAppTime double precision NOT NULL
	);

CREATE TABLE general_demand (
	n integer NOT NULL,
	m integer NOT NULL,
	p double precision NOT NULL,
	ratioEfficiency double precision NOT NULL,
	effAllocationRevenue double precision NOT NULL,
	effAllocationTime double precision NOT NULL,
	effAllocWEViolation double precision NOT NULL,
	effAllocWERelativeViolations double precision NOT NULL,
	effAllocEFViolation double precision NOT NULL,
	wfAllocationRevenue double precision NOT NULL,
	wfAllocationTime double precision NOT NULL,
	wfAllocWEViolation double precision NOT NULL,
	wfAllocWERelativeViolations double precision NOT NULL,
	wfAllocEFViolation double precision NOT NULL,
	wfMaxWEQRevenue double precision NOT NULL,
	wfMaxWEQTime double precision NOT NULL
	);

